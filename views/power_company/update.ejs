<% include ../partials/header %>
    <div class="make-row container">
        <h1 class="table-header">Update Power Company</h1>
            <form class="add-new-form" style="align-items: center;" action="/powercompany/updating" method="post">
                <div class="add-new-form-item">
                <label>PID</label><input class="form-control" type="text" name="pid" value="<%=result.pid%>" readonly></input></div>
                <div class="add-new-form-item">
                <label>PNAME</label><input class="form-control" type="text" name="pname" value="<%=result.pname%>"></input></div>
                <div class="add-new-form-item">
                <label>Total Power</label><input class="form-control" type="text" name="totalpower" value="<%=result.totalpower%>"></input></div>
                <div class="add-new-form-item">
                <label>Type</label><input class="form-control" type="text" name="type" value="<%=result.type%>"></input></div>
                <div class="add-new-form-item">
                <label>State</label><input class="form-control" type="text" name="state" value="<%=result.state%>"></input></div>
                <div class="add-new-form-item">
                <input type="submit" class="btn btn-success btn-block" value="Submit"></input>
                </div>
            </form>
    </div>
<% include ../partials/footer %>