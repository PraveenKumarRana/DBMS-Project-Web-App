<% include ../partials/header %>
    <div class="consumer-profile-page container">
        <h2 class="table-header">Welcome back, &nbsp;<%=consumer[0].cname%></h2>
        <div class="profile-page">
            <div class="user-info-container">
                <img src="https://cdn.pixabay.com/photo/2017/07/18/23/23/user-2517433_960_720.png" alt="User-icon" height="200px" width="200px" style="border-radius: 50%; box-shadow: 0 0 10px rgba(0, 0, 0, 0.493);"/>
                <h1 style="padding:5px; text-align: center; padding-top: 20px;"><%=consumer[0].cname%></h1>
            </div>
            <div class="user-info-container">
                <!-- <p><b>Name : </b><%=consumer[0].cname%></p> -->
                <p><b>Consumer ID : </b><span id="cid"><%=consumer[0].cid%></span></p>
                <p><b>Phone No : </b><%=consumer[0].phone%></p>
                <p><b>Email Id : </b><%=consumer[0].email%></p>
                <p><b>Boardname : </b><%=consumer[0].boardname%></p>
                <p><b>Sub Division : </b><%=consumer[0].subdiv%></p>
                <p><b>Division : </b><%=consumer[0].divis%></p>
                <p><b>City : </b><%=consumer[0].city%></p>
                <p><b>Address : </b></p>
                <%consumer.forEach(function(consumer){%>
                    <li><%=consumer.address%></li>
                <%})%>
            </div>
        </div>
    </div>
<% include ../partials/footer %>